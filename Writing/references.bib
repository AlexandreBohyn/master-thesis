	file = {Snapshot:C\:\\Users\\alexa\\Zotero\\storage\\CDDQR48W\\00224065.2016.html:text/html;Snapshot:C\:\\Users\\alexa\\Zotero\\storage\\AD22MZKS\\00224065.2016.html:text/html}
}

